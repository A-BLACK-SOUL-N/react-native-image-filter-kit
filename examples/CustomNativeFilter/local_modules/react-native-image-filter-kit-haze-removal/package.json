{
  "description": "Example haze removal filter for iOS & Android",
  "keywords": [
    "react-native",
    "image-filter"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "name": "react-native-image-filter-kit-haze-removal",
  "author": "iyegoroff <iegoroff@gmail.com>",
  "bugs": {
    "url": "https://github.com/iyegoroff/react-native-image-filter-kit/issues"
  },
  "homepage": "https://github.com/iyegoroff/react-native-image-filter-kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/react-native-image-filter-kit.git"
  },
  "version": "0.0.1",
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "@babel/runtime": "*",
    "react-native-image-filter-kit": "*"
  },
  "scripts": {
    "build": "rm -rf dist && ../../../../node_modules/.bin/tsc -p ./src",
    "watch": "../../../../node_modules/.bin/tsc -p ./src -w",
    "build:all": "npm run build"
  },
  "files": [
    ".dockerignore",
    "android",
    "ios",
    "src",
    "dist",
    "*.md",
    "*.podspec"
  ],
  "rnpm": {
    "commands": {
      "postlink": "npx iyegoroff/react-native-asset#with-key -ios-a ./node_modules/react-native-image-filter-kit-haze-removal/ios/Resources -k react-native-image-filter-kit-haze-removal",
      "postunlink": "npx iyegoroff/react-native-asset#with-key -ios-a -k react-native-image-filter-kit-haze-removal"
    }
  }
}