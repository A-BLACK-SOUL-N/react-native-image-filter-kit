{
  "infer-blacklist-path-regex": [
    ".*/react-native/.*",
    ".*/ScriptC_.*",
    ".*/Bolts/.*",
    ".*/yoga/.*",
    ".*/React/.*",
    ".*/React-\\w+/.*",
    ".*/Folly/.*",
    ".*/glog/.*",
    ".*/DoubleConversion/.*",
    ".*/boost-for-react-native/.*",
    ".*/NSMutableArray.m"
  ],

  "skip-analysis-in-path": [
    "Pods/Folly",
    "Pods/Bolts",
    "Pods/bolts-for-react-native",
    "Pods/DoubleConversion",
    "Pods/glog"
  ]
}
